<Application x:Class="SkinTouch.Mapping.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SkinTouch.Mapping"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="UI/Styles/Button.xaml" />
                <ResourceDictionary Source="UI/Styles/CheckBox.xaml" />
                <ResourceDictionary Source="UI/Styles/RadioButton.xaml" />
                <ResourceDictionary Source="UI/Styles/TextBox.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
